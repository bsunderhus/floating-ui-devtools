{
  "name": "floating-ui-devtools",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "build:lib": "vite build --config vite.lib.config.ts",
    "type-check": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@floating-ui/dom": "1.2.0",
    "@fluentui/react-components": "^9.41.0",
    "@fluentui/react-icons": "^2.0.223",
    "@fluentui/react-positioning": "^9.10.1",
    "@griffel/react": "^1.5.18",
    "react": "^17",
    "react-dom": "^17",
    "react-json-view": "^1.21.3",
    "scheduler": "0.20.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.251",
    "@types/node": "^20.9.4",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/scheduler": "^0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "5.0.4",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3"
  },
  "packageManager": "yarn@4.0.2",
  "files": [
    "lib"
  ],
  "main": "./lib/floating-ui-devtools.cjs",
  "module": "./lib/floating-ui-devtools.js",
  "typings": "./lib/floating-ui-devtools.d.ts",
  "exports": {
    ".": {
      "types": "./lib/floating-ui-devtools.d.ts",
      "import": "./lib/floating-ui-devtools.js",
      "require": "./lib/floating-ui-devtools.umd.cjs"
    }
  }
}
